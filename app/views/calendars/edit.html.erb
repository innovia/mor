<h2>Editing Calendar</h2>

<%= error_messages_for :calendar %>

<% form_for(:calendar, :url => object_url, :html => { :method => :put }) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%=submit_tag "Update"%>
  </p>
<% end %>

<br/>

<%= link_to 'Show', object_url %>
