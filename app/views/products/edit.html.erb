<h2>Editing Product</h2>

<%= error_messages_for :product %>

<% form_for(:product, :url => object_url, :html => {:multipart => true, :method => :put}) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%=submit_tag "Update"%>
  </p>
<% end %>

