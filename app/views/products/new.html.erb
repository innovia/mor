<h2>New Product</h2>

<%= error_messages_for :product %>

<% form_for(:product, :html => {:multipart => true},  :url => collection_url) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>
<br/>
<%= link_to 'Back', collection_url %>