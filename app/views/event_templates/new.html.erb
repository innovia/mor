<%= javascript_include_tag 'event_templates' %>
<div id="body">
<h2>New Class Template</h2>

<%= error_messages_for :event_template %>
<% form_for(:event_template, :url => collection_url) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<br/>
<%= link_to '<< Back', collection_url %>
</div>